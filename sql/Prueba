DROP TABLE IF EXISTS Inmueble;
CREATE TABLE Inmueble (
	Id int,
	Operacion varchar(10),
	Tipo varchar(10),
	Localidad varchar(50),
	Zona varchar(30),
	PrecioUSD bigint,
	PrecioARS bigint,
	DomicilioCalle varchar(20),
	DomicilioAltura bigint,
	Descripcion text,
	Servicios varchar(30)
);

